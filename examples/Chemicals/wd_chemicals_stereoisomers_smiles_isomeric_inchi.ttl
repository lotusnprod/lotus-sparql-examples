ex:wd_chemicals_stereoisomers_smiles_isomeric_inchi a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
rdfs:comment "Stereoisomers with isomeric SMILES."@en ;
sh:prefixes _:sparql_examples_prefixes;
sh:select """
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title: Stereoisomers with isomeric SMILES
SELECT * WHERE {
    ?structure wdt:P31 wd:Q59199015.
    ?structure wdt:P2017 ?smiles.
    ?structure wdt:P234 ?inchi.
}
""";
schema:target <https://query.wikidata.org/sparql> ;
