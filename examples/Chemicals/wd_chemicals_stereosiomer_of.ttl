@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://lotus.nprod.net/lotus-sparql-examples/Chemicals/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .

ex:wd_chemicals_stereosiomer_of a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
rdfs:comment "Pairs of stereoisomers present on Wikidata."@en ;
sh:prefixes _:sparql_examples_prefixes;
sh:select """
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title: Pairs of stereoisomers present on Wikidata
SELECT ?structure_1 ?structure_2 WHERE {
    ?structure_1 wdt:P3364 ?structure_2.
}
""";
schema:target <https://query.wikidata.org/sparql> ;
dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
schema:keywords "chemicals" ;
dc:creator "Lotus Contributors" .
