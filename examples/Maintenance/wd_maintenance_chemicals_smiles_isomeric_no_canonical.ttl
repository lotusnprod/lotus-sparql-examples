@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://lotus.nprod.net/lotus-sparql-examples/Chemicals/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .

ex:wd_maintenance_chemicals_smiles_isomeric_no_canonical a sh:SPARQLExecutable, sh:SPARQLSelectExecutable ;
rdfs:comment "Chemicals with isomeric SMILES but no canonical SMILES."@en ;
sh:prefixes _:sparql_examples_prefixes;
sh:select """
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title: Chemicals with isomeric SMILES but no canonical SMILES
SELECT ?structure ?smiles_isomeric WHERE {
    VALUES ?classes {
        wd:Q113145171
        wd:Q59199015
    }
    ?structure wdt:P2017 ?smiles_isomeric.
    ?structure wdt:P31 ?classes.
    OPTIONAL { ?structure wdt:P233 ?smiles_canonical. }
    FILTER(!(BOUND(?smiles_canonical)))
}
""";
schema:target <https://query.wikidata.org/sparql> ;
dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
schema:keywords "maintenance", "chemicals" ;
dc:creator "Lotus Contributors" .
