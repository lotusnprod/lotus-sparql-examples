@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <https://lotus.nprod.net/lotus-sparql-examples/Taxa/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix spex: <https://purl.expasy.org/sparql-examples/ontology#> .

<https://purl.example.39_wd_species_containing_all_compounds> a sh:SPARQLExecutable;
  rdfs:comment " Which taxa contain all of the following structures?", "title: Which species contain all of the following structures?";
  rdfs:label " Species containing all compounds";
  sh:prefixes _:sparql_examples_prefixes;
  sh:select """PREFIX idsm: https://idsm.elixir-czech.cz/sparql/endpoint/
PREFIX sachem: http://bioinfo.uochb.cas.cz/rdf/v1.0/sachem#
PREFIX wd: http://www.wikidata.org/entity/
PREFIX wdt: http://www.wikidata.org/prop/direct/
PREFIX xsd: http://www.w3.org/2001/XMLSchema#
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX sachem: <http://bioinfo.uochb.cas.cz/rdf/v1.0/sachem#>
PREFIX idsm: <https://idsm.elixir-czech.cz/sparql/endpoint/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT DISTINCT ?taxon ?taxon_name
WITH {
  SELECT DISTINCT ?structure ?smiles WHERE {
    SERVICE idsm:wikidata {
      VALUES ?smiles {
        \"C=C[C@H]1[C@@H](OC=C2C1=CCOC2=O)O[C@H]3[C@@H]([C@H]([C@@H]([C@H](O3)CO)O)O)O\"                             # Gentiopicrin
        \"C=C[C@H]1[C@@H](OC=C2[C@]1(CCOC2=O)O)O[C@H]3[C@@H]([C@H]([C@@H]([C@H](O3)CO)O)O)O\"                        # Swertiamarin
        \"C=C[C@@H]1[C@@H]2CCOC(=O)C2=CO[C@H]1O[C@H]3[C@@H]([C@H]([C@@H]([C@H](O3)CO)O)O)OC(=O)C4=C(C=C(C=C4O)O)O\"  # Amarogentin
        \"COC1=CC(=C2C(=C1)OC3=C(C=CC(=C3C2=O)O[C@H]4[C@@H]([C@H]([C@@H]([C@H](O4)CO)O)O)O)O)O\"                     # Swertianolin
      }
      # Exact match similarity (allows for different SMILES representations)
      ?structure sachem:similarCompoundSearch [
        sachem:query ?smiles;
        sachem:cutoff \"1.0\"^^xsd:double
      ].
    }
  }
} AS %structures 

WITH {
  SELECT (COUNT(DISTINCT ?smiles) AS ?targetCount) WHERE {
    INCLUDE %structures
  }
} AS %target_count 

WITH {
  SELECT DISTINCT ?taxon ?taxon_name ?structure WHERE {
    INCLUDE %structures
    ?structure wdt:P703 ?child.
    ?child (wdt:P171*) ?taxon.
    ?taxon wdt:P225 ?taxon_name.
    ?taxon wdt:P105 ?taxon_rank.
    FILTER (?taxon_rank = wd:Q7432)  # species
  }
} AS %taxon_compounds 

WITH {
  SELECT ?taxon ?taxon_name (COUNT(DISTINCT ?structure) AS ?structureCount) WHERE {
    INCLUDE %taxon_compounds
  }
  GROUP BY ?taxon ?taxon_name
} AS %taxon_structure_counts 

WHERE {
  INCLUDE %taxon_structure_counts
  INCLUDE %target_count
  FILTER(xsd:integer(?structureCount) >= xsd:integer(?targetCount))
}
LIMIT 10000""";
  schema:keywords "Natural products", "Structural similarity", "Taxa";
  schema:target " https://query.wikidata.org/bigdata/namespace/wdq/sparql" .
